# API Versioning Example Configuration
# Copy this to .env.local and customize for your needs

# Server Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# API Versioning Configuration
VERSIONING_ENABLED=true
VERSIONING_SUPPORTED_VERSIONS=v1,v2,v3
VERSIONING_DEFAULT_VERSION=v1
VERSIONING_HEADER=API-Version
VERSIONING_PREFIX=true
VERSIONING_STRICT_MODE=false

# Mock Data Configuration
MOCK_DATA_PATH=./data
ENABLE_CRUD=true
DEFAULT_DELAY=0

# Security Configuration (relaxed for development)
AUTH_ENABLED=false
CORS_ORIGINS=*

# Logging
LOG_LEVEL=debug
LOG_FORMAT=simple

# Admin
ADMIN_ENABLED=true
